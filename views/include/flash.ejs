<% if (success && success.lenght) {%>
<div class="alert alert-success alert-dismissible fade show col-6 offset-3"
role="alert">
<%= success>
<button
type="button"
class="btn-close"
data-bs-dismiss="alert"
>
    
</button>
</div>
<%}%>


<% if (error && error.lenght) {%>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3"
    role="alert">

    <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    >
        
    </button>
    </div>
    <%}%>